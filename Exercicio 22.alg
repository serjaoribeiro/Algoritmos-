algoritmo "Exercicio 22"
//Leia um valor de comprimento em jardas e apresente-o convertido em metros.
var
   j, m:real
inicio
      escreva("Quantas jardas tem do seu trabalho ate sua casa? ")
      leia(j)
      m <- 0.91 * j
      escreva("Entao do seu trabalho ate sua casa tem:",m," metros!")
fimalgoritmo
