algoritmo "Exercicio 6"
//Leia uma temperatura em graus Celsius e apresente-a convertida em graus Fahrenheit.

var
   C, F:Real


inicio
      escreval("Qual sua temperatura em Graus C° atual?")
      leia(C)
      F <- (C * 1.8)+32
      escreval("Sua temperatura em graus Fahrenheit e",F ,"°" )

fimalgoritmo
