algoritmo "Exercico 30"
//Leia um valor em real e a cotac¸ao do d ˜ olar. Em seguida, imprima o valor correspondenteem dolares
var
   reall, cotacao:real

inicio
      escreva("Digite a quantidade em reall! ")
      leia(reall)
      escreva("cotacao: ")
      leia(cotacao)
      escreva("Voce tem:", reall * cotacao, " R$ dolares ")
fimalgoritmo


